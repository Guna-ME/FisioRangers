Table Pessoa {
  Cod_Pessoa int [primary key]
  Nome char(50) 
  Celular int(11) 
  Cidade char(50) 
}

Table Usuario {
  Cod_Usuario int [primary key]
  Tipo char(50) 
  Nome char(50) 
  Telefone int(11) 
}

Table Sala {
  Cod_Sala int [primary key]
  Local char(50) 
  Disposicao char(1) 
}

Table Agenda {
  Cod_agenda int [primary key]
  Cod_Fisio int 
  Cod_Paciente int
  Cod_Usuario int
  Cod_Sala int 
  Cod_Servico int
  Horario datetime 
  Cancelamento char(1) 
}

Table Servico {
  Cod_Servico int [primary key]
  Tipo_Servico char(50) 
  Valor float(6.2) 
  Cod_Pagamento int
  Cod_Fisio int 
}

Table Pagamento {
  Cod_Pagamento int [primary key]
  Cod_Tipo int
  Multa_Cancelamento float(6.2) 
}

Table Tipo_Pagamento {
  Cod_Tipo int [primary key]
  Tipo_Pagamento char(10) 
}

Table log {
  Data datetime 
  Nome_Usuario char(50)
  Tipo_Operacao char(50)
  Descricao char(100)
}

ref: Pessoa.Cod_Pessoa < Agenda.Cod_Paciente
ref: Pagamento.Cod_Pagamento < Servico.Cod_Pagamento
ref: Usuario.Cod_Usuario < Agenda.Cod_Usuario
ref: Sala.Cod_Sala - Agenda.Cod_Sala
ref: Pessoa.Cod_Pessoa < Agenda.Cod_Fisio
ref: Servico.Cod_Servico > Agenda.Cod_Servico
ref: Tipo_Pagamento.Cod_Tipo > Pagamento.Cod_Tipo
